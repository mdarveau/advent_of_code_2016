data = [
  ""
  ""
  ""
  ""
  ""
]

position =
  x: 0
  y: 2

pad = [
  [ null, null, 1, null, null ]
  [ null, 2, 3, 4, null ]
  [ 5, 6, 7, 8, 9 ]
  [ null, 'A', 'B', 'C', null ]
  [ null, null, 'D', null, null ]
]

resolve = ( position )->
  pad[position.y]?[position.x]

for instruction in data
  for x in [0..(instruction.length - 1)]
    newY = null
    newX = null
    position.y += 1 if instruction.charAt( x ) == 'D' and resolve( {x: position.x, y: position.y + 1} )?
    position.y -= 1 if instruction.charAt( x ) == 'U' and resolve( {x: position.x, y: position.y - 1} )?
    position.x -= 1 if instruction.charAt( x ) == 'L' and resolve( {x: position.x - 1, y: position.y} )?
    position.x += 1 if instruction.charAt( x ) == 'R' and resolve( {x: position.x + 1, y: position.y} )?

#    console.log "#{instruction.charAt( x )} -> #{resolve( position )} (#{JSON.stringify( position )})"

  console.log resolve( position )
