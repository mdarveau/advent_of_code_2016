data = [
  ""
  ""
  ""
  ""
  ""
  ]

position =
  x: 1
  y: 1

resolve = ( position )->
  (position.x+1) + ((position.y)*3)

for instruction in data
  for x in [0..(instruction.length-1)]
#    console.log instruction.charAt(x)
    position.y += 1 if instruction.charAt(x) == 'D'
    position.y -= 1 if instruction.charAt(x) == 'U'
    position.x -= 1 if instruction.charAt(x) == 'L'
    position.x += 1 if instruction.charAt(x) == 'R'

    position.x = 0 if position.x < 0
    position.y = 0 if position.y < 0
    position.x = 2 if position.x > 2
    position.y = 2 if position.y > 2

#    console.log position

  console.log resolve(position)
